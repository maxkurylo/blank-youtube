{
    "name": "Blank YouTube",
    "description": "Make main YouTube section blank",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/", "https://youtube.com/"],
          "run_at": "document_start",
          "css": ["content.css"]
        }
    ]
}